<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".QuizPageFiveActivity">

    <TextView
        android:id="@+id/id_tv_question_bigMoney"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_margin="32dp"
        android:layout_marginTop="128dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/is_the_amount_of_money_you_are_looking_for_over_50_000"
        android:textColor="@color/dark_green"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="116dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/id_tv_yes_bigMoney"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="64dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/yes_it_is"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/id_tv_no_bigMoney"
        app:layout_constraintTop_toBottomOf="@id/id_tv_question_bigMoney"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="263dp" />

    <TextView
        android:id="@+id/id_tv_no_bigMoney"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="48dp"
        android:layout_marginStart="32dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/no_i_need_less"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/id_tv_yes_bigMoney"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/id_tv_question_bigMoney"
        tools:layout_editor_absoluteX="267dp"
        tools:layout_editor_absoluteY="244dp" />

    <CheckBox
        android:id="@+id/id_checkBox_yes_bigMoney"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/id_checkBox_no_bigMoney"
        app:layout_constraintTop_toBottomOf="@id/id_tv_yes_bigMoney"
        android:layout_marginTop="32dp"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="326dp" />

    <CheckBox
        android:id="@+id/id_checkBox_no_bigMoney"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginTop="32dp"
        android:layout_marginStart="100dp"
        app:layout_constraintStart_toEndOf="@id/id_checkBox_yes_bigMoney"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/id_tv_no_bigMoney"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="343dp" />

    <ImageView
        android:id="@+id/id_img_bigMoney"
        android:layout_width="176dp"
        android:layout_height="262dp"
        android:layout_marginTop="64dp"
        app:srcCompat="@drawable/big_money"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="277dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/id_img_smallMoney"
        app:layout_constraintTop_toBottomOf="@id/id_checkBox_yes_bigMoney"/>


    <ImageView
        android:id="@+id/id_img_smallMoney"
        android:layout_width="176dp"
        android:layout_height="262dp"
        android:layout_marginTop="64dp"
        app:srcCompat="@drawable/small_money"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="250dp"
        app:layout_constraintStart_toEndOf="@id/id_img_bigMoney"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/id_checkBox_no_bigMoney"/>

    <Button
        android:id="@+id/id_btn_quizFiveNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        android:text="@string/next"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="535dp" />


</androidx.constraintlayout.widget.ConstraintLayout>